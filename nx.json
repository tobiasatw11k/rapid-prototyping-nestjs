{
  "npmScope": "tss",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "projects": {
    "workshop-prototype": {
      "tags": ["scope:frontend"]
    },
    "workshop-prototype-e2e": {
      "tags": [],
      "implicitDependencies": ["workshop-prototype"]
    },
    "api": {
      "tags": ["scope:backend"]
    },
    "api-interfaces": {
      "tags": ["scope:lib"]
    },
    "shared": {
      "tags": ["scope:lib"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
